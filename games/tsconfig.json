{
	"compilerOptions": {
		"outDir": "./dist",    
		"module": "commonjs",
		"target": "esnext",
		"lib": ["esnext"],
		"preserveConstEnums": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"types": [
			"@cloudflare/workers-types",
			"node"
		] ,
		"resolveJsonModule": true
	},
	"include": ["src"],
  "exclude": ["node_modules"]
}
